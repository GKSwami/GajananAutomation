{
  "name": "lambdatest-playwright-automation",
  "version": "1.0.0",
  "description": "Playwright automation tests for LambdaTest Selenium Playground",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "playwright test",
    "test:local": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright test && playwright show-report",
    "test:chrome": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit"
  },
  "keywords": [
    "playwright",
    "automation",
    "lambdatest",
    "e2e-testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "dotenv": "^16.3.1"
  }
}

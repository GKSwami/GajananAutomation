# LambdaTest Credentials (for HyperExecute integration)
LAMBDA_USERNAME=your_username_here
LAMBDA_ACCESS_KEY=your_access_key_here

# Test Configuration
NODE_ENV=test
DEBUG=pw:api

# Report Settings
REPORT_PATH=./playwright-reports
VIDEO_ENABLED=true
SCREENSHOT_ENABLED=true
TRACE_ENABLED=true

# Timeout Settings (in milliseconds)
TIMEOUT=30000
NAVIGATION_TIMEOUT=30000

# Test Data
TEST_BASE_URL=https://www.lambdatest.com/selenium-playground
TEST_MESSAGE=Welcome to LambdaTest
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=your_password_here

# Logging
LOG_LEVEL=info
